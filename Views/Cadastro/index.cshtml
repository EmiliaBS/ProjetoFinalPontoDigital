<header>
    @{
        Html.RenderPartial("_HeaderNavBar");
        Layout = "_Layout";
    }
    <head>
        <link rel="stylesheet" href="../css/style-login.css"> 
    </head>
     <main>
        <div class="pessoa">
            <h1>LOGIN:</h1>
        </br>
            <form method="POST" action='@Url.Action("Logar", "Login")'>
                <div>
                    <label for="email">E-mail:</label>
                </br>
                <input id="email" type="text" maxlength="30" minlength="3" name="email"/>
                </div>
            </br>
               
                <div>
                    <label for="senha">Senha:</label>
                </br>
                    <input id="senha" type="text" maxlength="20" minlength="6" name="senha" />
                </div>

                <input type="submit" value="Acessar minha conta"/>

            </form>
        </div>
                <div class="cadastrando">
                <h2>CADASTRE-SE!!!</h2>
                </br>
                <form method="POST" action="/Cadastro/CadastroCliente">
                    <div>
                        <label for="nome">Nome completo:</label>
                    <br />
                        <input name="nome" id="nome" type="nome" maxlength="50" minlength="3" required />
                    <br />
                    </div>
                </br>
                    <div>
                        <label for="data">Data de nascimento</label>
                    <br />
                        <input type="date" name="data">
                    <br />
                    </div>
                </br>
                    <div>
                        <label for="telefone">Telefone para contato</label>
                    <br />
                        <input name="telefone" id="telefone" type="text" required />
                    <br />
                    </div>
                </br>
                 

                    <div>
                        <label for="rg">RG</label>
                    <br />
                        <input name="rg" id="rg" type="text" maxlength="12" minlength="9" required />
                    <br />
                    </div>
                </br>
                    <div>
                        <label for="cpf">CPF:</label>
                    <br />
                        <input name="cpf" id="cpf" type="text" maxlength="12" minlength="11" required />
                    <br />
                    </div>
                </br>
                    <div>
                        <label for="cep">Cep</label>
                    <br />
                        <input name="cep" id="cep" type="text" required />
                    <br />
                    </div>
                </br>
                    <div>
                        <label for="endereco">Endereço</label>
                    <br />
                        <input name="endereco" id="endereço" type="text" required /> 
                    <br />
                    </div>
                </br>
                    <div>
                        <label for="numero">N°</label>
                    <br />
                        <input name="numero" id="número" type="text" required />
                    <br />
                    </div>
                </br>
                    <div>
                        <label for="complemento">Complemento:</label>
                    <br />
                        <input name="complemento" id="complemento" type="text" maxlength="18" minlength="5" required />
                    <br />
                    </div>
                </br>
                    <div>
                        <label for="bairro">Bairro</label>
                    <br />
                        <input name="bairro" id="bairro" type="text" required />
                    <br />
                    </div>
                </br>
                    <div>
                        <label for="cidade">Cidade</label>
                    <br />
                        <input name="cidade" id="cidade" type="text" required />
                    <br />
                    </div>
                </br>
                    <div>
                        <label for="estado">Estado</label>
                    <br />
                        <input name="estado" id="estado" type="text" required />
                    <br />
                    </div>
                </br>
                    <div>
                        <label for="nomeEmpresa">Nome da empresa</label>
                    <br />
                        <input name="nomeEmpresa" id="nome" type="nome" maxlength="50" minlength="3" required />
                    <br />
                    </div>
                </br>
                    <div>
                        <label for="cnpj">CNPJ</label>
                    <br />
                        <input name="cnpj" id="cnpj" type="text" required />
                    <br />
                    </div>
                </br>
                    <div>
                        <label for="emai">E-mail </label>
                    <br />
                        <input id="email" type="email" maxlength="25" name="email" />
                    <br />
                    </div>
                </br>
                    <div>
                        <label for="senha">Senha</label>
                        <br />
                        <input id="senha" type="password" maxlength="20" minlength="3" name="senha"/>
                    <br />
                    </div>
                </br>
                    <div>
                        <input type="submit" value="Enviar" />
                    </div>
                </br>
            </form>
        </div>
    </main>
    
</header>